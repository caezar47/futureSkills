{% if view == 'articles_modal' %}
	<div class="{{prefix}}item  {{mod}}">
		<div class="{{prefix}}img  {{mod}}">
			<picture>
				<img src="" data-picture class="img-responsive" alt="">
			</picture>
		</div>
		<div class="{{prefix}}group  {{mod}}">
			<p class="{{prefix}}date  {{mod}}" data-date></p>
