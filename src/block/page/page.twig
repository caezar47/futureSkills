{% if view == "challenge" %}
	<section class="{{content_prefix}}content  {{content_mod}}" > 
		<div class="{{content_prefix}}container container  {{content_mod}}">
			{% if heading %}
			{% include "block.twig" with {
				view:   'header',
			} %}
			{% endif %}
			{% if content %}
				{% for content in content %}
				<div class="{{content_prefix}}group  {{content_mod}}  {{content.mod}}">
					{% if content.heading %}
					<h2 class="{{content_prefix}}heading  {{content_mod}}  {{content.mod}}"> 
						{{content.heading}}             
					</h2>
					{% endif %}
					{% if content.view == "adv" %}
						{# <div data-slick-challenge-adv-thumb>
							{% for value in content.card_list %}
								<div class="{{content.card_prefix}}item  {{content.card_mod}}-thumb">{{value.name}}</div>
							{% endfor %}
						</div>#}
						<div data-slick-challenge-adv  class="{{content_prefix}}grid  {{content_mod}}  {{content.mod}}">
							<div class="slick-btn-group">
								<button type="button" class="slick-btn  is--prev is--ch-adv  slick-arrow" style=""><span class="sr-only">Предыдущий слайд</span></button>
								<div class="slick-count"></div>
								<button type="button" class="slick-btn  is--next is--ch-adv slick-arrow" style=""><span class="sr-only">Следующий слайд</span></button>
							</div>
							{% include "../common/card/card.twig" with {
								view:   content.card_view,
								prefix: content.card_prefix,
								mod:    content.card_mod,
								list:   content.card_list,
							} %}
						</div>
					{% endif %}
					{% if content.view == "item" %}
						<div class="{{content_prefix}}group  {{content_mod}}  {{content.mod}}-panel" data-slick-challenge-group>
							<div class="slick-btn-group">
								<button type="button" class="slick-btn is--prev is--ch-item slick-arrow" style=""><span class="sr-only">Предыдущий слайд</span></button>
								<div class="slick-count"></div>
								<button type="button" class="slick-btn is--next is--ch-item slick-arrow" style=""><span class="sr-only">Следующий слайд</span></button>
							</div>
							{% for value in content.card_list %}
							<div class="slide--group">
								{% if value.name %}
									<h3 class="{{content_prefix}}heading  {{content_mod}}  {{content.mod}}-panel"> 
										{{value.name}}             
									</h3>
								{% endif %}
								{# <div data-slick-challenge-item-thumb> 
									{% include "../common/card/card.twig" with {
										view:   content.card_view,
										prefix: content.card_prefix,
										mod:    content.card_mod_thumb,
										list:   value.card_list,
									} %}
								</div>
								<div class="{{content_prefix}}grid  {{content_mod}}  {{content.mod}}" data-slick-challenge-item>
								#}
								<ul class="{{content_prefix}}grid  {{content_mod}}  {{content.mod}}  is--nav">
									{% include "../common/card/card.twig" with {
										view:   content.card_view,
										prefix: content.card_prefix,
										mod:    content.card_mod,
										list:   value.card_list_nav,
									} %}
								</ul>
								<div class="{{content_prefix}}grid  {{content_mod}}  {{content.mod}}">
									{% include "../common/card/card.twig" with {
										view:   content.card_view,
										prefix: content.card_prefix,
										mod:    content.card_mod,
										list:   value.card_list,
									} %}
								</div>
							</div>
							{% endfor %}
						</div>
					{% endif %}
					{% if content.view == "card" %}
						<div class="{{content_prefix}}group  {{content_mod}}  {{content.mod}}-panel" data-slick-challenge-card>
							{% include "../common/card/card.twig" with {
								view:   content.card_view,
								prefix: content.card_prefix,
								mod:    content.card_mod,
								list:   content.card_list,
							} %}
						</div>
					{% endif %}
					{% if content.view == "team" %}
						{% include "../common/card/card.twig" with {
							view:   content.card_view,
							prefix: content.card_prefix,
							mod:    content.card_mod,
							list:   content.card_list,
						} %}
					{% endif %}
					{% if content.view == "sponsors" %}
					<div class="{{content_prefix}}grid  grid__row  {{content_mod}}  {{content.mod}}">
						{% include "../common/card/card.twig" with {
							view:   content.card_view,
							prefix: content.card_prefix,
							mod:    content.card_mod,
							list:   content.card_list,
						} %}
					</div>
					{% endif %}
				</div>
				{% endfor %}
			{% endif %}
		</div>
	</section>
{% endif %}
{% if view == "challenge-modal" %}
	<section class="{{content_prefix}}content  {{content_mod}}  section" >
		<div id="nav" class="{{content_prefix}}group  {{content_mod}}">
           {#  <img src="{{path.img}}/bg/bg-sm.jpg" alt="" class="{{content_prefix}}bg  {{content_mod}}  is--top">
            <img src="{{path.img}}/bg/bg-lg.jpg" alt="" class="{{content_prefix}}bg  {{content_mod}}  is--bottom"> #}
            {% for b in bg %}
            	<img src="{{b.img}}" alt="" class="{{content_prefix}}bg  {{content_mod}}  {{b.mod}}"> 
            {% endfor %}
			<div class="{{content_prefix}}container container  {{content_mod}}">
			<div class="{{content_prefix}}nav-wrap  {{content_mod}}">
				<div class="{{content_prefix}}nav  {{content_mod}}">
					{% for nav in nav %}
						<div class="{{content_prefix}}group  {{content_mod}}-card  {{nav.mod}}" id="{{nav.id}}"> 
							<h2 class="{{content_prefix}}heading  {{content_mod}}"> 
								{{nav.name}}             
							</h2>
							<div class="{{content_prefix}}note  {{content_mod}}"> 
								{{nav.note}}             
							</div>
						</div>
					{% endfor %}
				</div>
			</div>
			<div class="{{content_prefix}}img  {{content_mod}}">
				<div class="arrow">
					<img src="{{path.img}}/page/сhallenge/modal/arrow-bottom.svg" alt="">
				</div>
				<div class="round"></div>
				<div class="center">
					<div class="center--img">
						{% for i in img %}
						<img src="{{i.img}}" alt="" class="{{i.mod}}">
						{% endfor %}
					</div>
					<div class="center--text">
						<p class="center--text1">Профессионал</p>
						<p class="center--text2">Новый профессионал</p>
						<p class="center--text3">Новые <br>профессионалы</p>
					</div>
				</div>
				{% for ic in icon %}
					<div class="icon  {{ic.mod}}">
						<img src="{{ic.img}}" alt="">
						<p>{{ic.name}}</p>
					</div>
				{% endfor %}
				<div class="text">
					<h6>Новые продукты</h6>
					<p>создаются командами, их социальными навыками и образом мышления (mindsets)</p>
				</div>
			</div>
		</div>
		</div>
	</section>
	{% for value in list %}
	<section class="{{content_prefix}}content  {{content_mod}}  section" ></section> 
	{% endfor %} 
	{# <section class="{{content_prefix}}content  {{content_mod}}  section" > 
		<div class="{{content_prefix}}container container  {{content_mod}}">
			
			</div>
			{% endif %}
			<div class="{{content_prefix}}img  {{content_mod}}">
				<img src="{{value.img}}" class="img-responsive" alt="">
			</div>
		</div>
	</section>
	{% endfor %} #}
{% endif %}
{% if view == "competencies" %}
	<section class="{{content_prefix}}content  {{content_mod}}" {% if bg %}style="background-image: url({{bg}});"{% endif %}>
		<div class="{{content_prefix}}container container  {{content_mod}}">
			{% include "block.twig" with {
				view:	'header',
			} %}
			<div class="{{content_prefix}}grid  grid__row  {{content_mod}}">
				{% include "../common/card/card.twig" with {
					view:	card_view,
					prefix:	card_prefix,
					mod:	card_mod,
					list:	card_list,                    
				} %}
			</div>
			<div class="{{content_prefix}}btn  is--more">
				{% include "../common/button/tpl.twig" with {
					view:       'tpl',
					tag:	    btn_tag,
					link:       btn_link,
					attr:       btn_attr,
					modal:      btn_modal,
					prefix:     btn_prefix,
					mod:        btn_mod,
					elem:       btn_elem,
					icon:       btn_icon,
					icon_svg:	btn_icon_svg,
					name:       btn,
				} %}
			</div>
		</div>
	</section>
{% endif %}
{% if view == "competencies_card" %}
	<section class="{{content_prefix}}content  {{content_mod}}" {% if bg %}style="background-image: url({{bg}});"{% endif %}>
		<div class="{{content_prefix}}container container  {{content_mod}}">
			{% include "block.twig" with {
				view:   'header',
			} %}
			{% if content %}
				{% for content in content %}
				<div class="{{content_prefix}}group  {{content_mod}}  {{content.mod}}">
					<h2 class="{{content_prefix}}heading  {{content_mod}}  {{content.mod}}"> 
						{{content.heading}}             
					</h2>
					{% if content.tabs %}
						{% include "../common/tabs/tpl.twig" with {
							prefix: content.tabs_prefix,
							list:   content.tabs,                    
						} %}
					{% endif %}
					{% if content.gallery %}
						<div data-slick-gallery>
							{% for gallery in content.gallery %}
							   <div class="{{content_prefix}}img  {{content_mod}}  {{content.mod}}">
									<picture>
										<img src="{{gallery.img}}" class="img-responsive" alt="">
									</picture>
							   </div> 
							{% endfor %}
						</div>
						<div data-slick-gallery-thumb>
							{% for gallery in content.gallery %}
							   <div class="{{content_prefix}}img  {{content_mod}}  {{content.mod}}-thumb">
									<picture>
										<img src="{{gallery.img}}" class="img-responsive" alt="">
									</picture>
							   </div> 
							{% endfor %}
						</div>
					{% endif %}
					{% if content.team %}
						<div class="{{content_prefix}}grid  grid__row  {{content_mod}}  {{content.mod}}">
							{% include "../common/card/card.twig" with {
								view:   content.team_view,
								prefix: content.team_prefix,
								mod:    content.team_mod,
								list:   content.team,                    
							} %}
						</div>
					{% endif %}
				</div>
				{% endfor %}
			{% endif %}
		</div>
	</section>
{% endif %}
