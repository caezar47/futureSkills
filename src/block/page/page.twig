{% if view == "landing" %}
	{% for content in content %}
	<section class="{{content_prefix}}content  {{content.mod}}" 
		{% if content.bg %}
			style="background-image: 
			{% for bg in content.bg %}
				{% if bg.img %}
				url({{bg.img}})
				{% endif %}
				{% if bg.gradient %}
				{{bg.gradient}}
				{% endif %}
				{% if loop.last %}{% else %},{% endif %}
			{% endfor %}
			;" 
		{% endif %}
	>
		<div class="{{content_prefix}}container container  {{content.mod}}">
			{% if content.heading %}
			<{{content.heading_tag}} class="{{content_prefix}}heading  {{content.mod}}">
				{{content.heading}}
			</{{content.heading_tag}}>
			{% endif %}	
			{% if content.img %}	
			<div class="{{content_prefix}}img  {{content.mod}}">
				<img src="{{content.img}}" class="img-responsive" alt="">
			</div>	
			{% endif %}	
			{% if content.text %}	
			<div class="{{content_prefix}}text  {{content.mod}}">
				{{content.text}}
			</div>	
			{% endif %}			
		</div>
	</section>
	{% endfor %}
{% endif %}
